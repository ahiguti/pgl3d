#!/usr/bin/env pxc

public namespace gaussian;
import common -;
import numeric::fpmath -;

const s = 1.5;
const k = 15;
const pi = 3.1415926535897932384626;
varray{double} vs;
for (int z: -k .. k + 1) {
  const zd = static_cast{double}(z);
  const v = exp(-zd * zd / (2.0 * s * s)) / sqrt(2.0 * pi * s * s);
  vs.push_back(v);
  println(to_string(v) + ",");
}
